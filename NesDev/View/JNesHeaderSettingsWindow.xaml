		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="100" />
			<ColumnDefinition Width="*" />
		</Grid.ColumnDefinitions>

		<ListBox>
			<TextBlock FontWeight="Bold">Selection</TextBlock>
			<ListBoxItem HorizontalAlignment="Center">Hello</ListBoxItem>
			<ListBoxItem HorizontalAlignment="Center">Hello</ListBoxItem>
			<ListBoxItem HorizontalAlignment="Center">Hello</ListBoxItem>
			<ListBoxItem HorizontalAlignment="Center">Hello</ListBoxItem>
		</ListBox>
	</Grid>
</Window>
	